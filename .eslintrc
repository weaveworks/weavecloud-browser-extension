{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "chrome": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "new-cap": [2, { "capIsNewExceptions": ["List", "OrderedMap"] }],
    "react/jsx-closing-bracket-location": 0,
    "react/prop-types": 0
  },
  "plugins": [
    "react"
  ]
}
